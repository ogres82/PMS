  <Model/>
  <View id="eventView">
    <ClientEvent name="onReady">tcCore.loadJs($url(&quot;>/Hplus/js/bootstrap.min.js?v=3.3.6&quot;));&#xD;
tcCore.loadJs($url(&quot;>/Hplus/js/content.min.js?v=1.0.0&quot;));&#xD;
